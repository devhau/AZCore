@model FormUpdateDevice
    <div class="az-update row" id="FormUpdateArea" role="form">
        <div class="form-group col-md-6">
            <az-hotel-select model="Model.Data" func="(item)=>((DeviceModel)item).HotelID" label="Tên phòng" />
        </div>
        <div class="form-group col-md-6">
            <az-device-common-select model="Model.Data" func="(item)=>((DeviceModel)item).DeviceID" label="Tên thiết bị" attr="autofocus" />
        </div>
        <div class="form-group col-md-12">
            <az-text-area-model model="Model.Data" func="(item)=>((DeviceModel)item).Note" label="Ghi chú" />
        </div>
    </div>
<az-script>

</az-script>