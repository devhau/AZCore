@model FormSystemCode
<az-script module="SystemCode.js"/>
<az-module-layout script-random='(id)=>$"SystemCode(\".{id}\");"' buttons="Model.GetButtonInfo()" class="az-manager" attr='data-form-size="az-modal-xl"'>
        <div class="az-data-table">
            <az-table data="Model.Data" FunKey="(item)=>item.Id" columns="Model.Columns" fun-where-edit="(item)=>item.PrefixIndex==0" fun-where-remove="(item)=>item.PrefixIndex==0" pagination="Model" is-edit="Model.HasPermission(PE.Role_Edit)" is-delete="Model.HasPermission(PE.Role_Remove)"></az-table>
        </div>
        <div class="az-bottom-manager">
            <az-pagination pagination="Model" />
        </div>
</az-module-layout>
