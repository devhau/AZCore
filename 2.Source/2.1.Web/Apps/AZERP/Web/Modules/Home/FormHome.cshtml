                    <h3>Nhóm tìm kiếm nhiều</h3>
                </div>
            </div>
        </div>
        <div class="row">
            @for (var i = 1; i < 20; i++)
            {
                <az-component component="Component/CatagoryJobInfo" model="i" />
            }
        </div>
    </div>
</div>
