@model FormUpdateUser
@using AZ.Web.Entities
<div class="az-update row" id="FormUpdateUser" role="form">
    <div class="form-group col-md-6">
        <az-model-input model="Model.Data" func="(item)=>((UserModel)item).FullName" label="Họ tên" />
    </div>
    <div class="form-group col-md-6">
        <az-model-input model="Model.Data" func="(item)=>((UserModel)item).Email" label="Email" />
    </div>
    <div class="form-group col-md-6">
        <az-model-input model="Model.Data" func="(item)=>((UserModel)item).PhoneNumber" label="Số điện thoại" />
    </div>
    <div class="form-group col-md-6">
        <az-model-input model="Model.Data" func="(item)=>((UserModel)item).Status" type="checkbox" label="Trạng thái" />
    </div> 
    <div class="form-group col-md-6">
        <az-model-input model="Model.Data" func="(item)=>((UserModel)item).Password" type="password" label="Mật khẩu" />
    </div>
</div>
<az-script>

</az-script>
