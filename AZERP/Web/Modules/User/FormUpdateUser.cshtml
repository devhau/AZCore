@model FormUpdateUser
<div class="az-update row" id="FormUpdateUser" role="form">
    <div class="form-group col-md-6">
        <az-text-model model="Model.Data" func="(item)=>((UserModel)item).FullName" label="Họ tên" />
    </div>
    <div class="form-group col-md-6">
        <az-text-model model="Model.Data" func="(item)=>((UserModel)item).Email" label="Email" />
    </div>
    <div class="form-group col-md-6">
        <az-text-model  model="Model.Data" func="(item)=>((UserModel)item).PhoneNumber" label="Số điện thoại" />
    </div>
    <div class="col-md-6">
    <div class="form-group">
        <az-status-select model="Model.Data" func="(item)=>((UserModel)item).Status" label="Trạng thái" />
    </div>
</div>
    <div class="form-group col-md-6">
        <az-password-model model="Model.Data" func="(item)=>((UserModel)item).Password" label="Mật khẩu" />
    </div>
</div>
<az-script>

</az-script>
