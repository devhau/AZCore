@model FormUpdateSystemCode
    <div class="az-update row" role="form">
        <div class="col-md-12">
            <div class="form-group">
                <az-system-code-select model="Model.Data" func="(item)=>((SystemCodeModel)item).Key" label="Key" is-enable="Model.Data==null" />
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <az-text-model model="Model.Data" func="(item)=>((SystemCodeModel)item).Prefix" label="Tiền tố" value-default='"VNI"' />
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <az-number-model type="Integer" max-length="4" model="Model.Data" func="(item)=>((SystemCodeModel)item).Len" label="Chiều dài" value-default="5" />
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <az-status-select model="Model.Data" func="(item)=>((SystemCodeModel)item).Status" label="Trạng thái" />
            </div>
        </div>
    </div>
<az-script>
</az-script>