@model FormPermission
<az-script module="ViewPermission.js" />
<az-layout class="row" script-random='(id)=>$"ViewPermission(\".{id}\");"'>
    <div class="col-12 search-permssion">
        <form>
            <input type="hidden" name="h" value="role" />
            <az-role-select model="@Model" func="(item)=>((FormPermission)item).RoleId" where-func="(Item)=>((RoleModel)Item).Status==AZCore.Database.EntityStatus.Active&&((RoleModel)Item).IsDelete==false" label="Vai trò" />
        </form>
    </div>
    <div class="col-12">
        <label>Quyền của vai trò</label>
        @Model.ViewChild("ViewPermission2", Model.dataView)
    </div>
</az-layout>