@model FormTenant
<az-module-layout script-random='(id)=>$"$(\".{id}\").AZManager();"' buttons="Model.GetButtonInfo()" class="az-manager" attr='data-form-size="az-modal-xl"'>
    <div class="az-data-table">
        <az-table data="Model.Data" FunKey="(item)=>((TenantModel)item).Id" columns="Model.Columns" pagination="Model" is-edit="Model.HasPermission(PE.Tenant_Edit)" is-delete="Model.HasPermission(PE.Tenant_Remove)"></az-table>
    </div>
    <div class="az-bottom-manager">
        <az-pagination pagination="Model" />
    </div>
</az-module-layout>
