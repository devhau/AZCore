@model FormUpdateCategory
<div class="az-update row" id="FormUpdateCategory" role="form">

    <div class="form-group col-md-12">
        <az-code-model model="Model.Data" func="(item)=>((CategoryModel)item).Code" label="Mã code" />
    </div>
    <div class="form-group col-md-12">
        <az-Category-select model="Model.Data" func="(item)=>((CategoryModel)item).ParentId" label="Nhóm cha" />
    </div>
    <div class="form-group col-md-12">
        <az-text-model model="Model.Data" func="(item)=>((CategoryModel)item).Name" label="Tên nhóm" />
    </div>
    <div class="form-group col-md-12">
        <az-status-select model="Model.Data" func="(item)=>((CategoryModel)item).Status" label="Trạng thái" />
    </div>
</div>
<az-script>

</az-script>