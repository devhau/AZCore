@model FormUpdateCommonService
    <div class="az-update row" id="FormUpdateCommonService" role="form">
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((CommonServiceModel)item).CommonServiceCode" label="Mã dịch vụ" attr="autofocus" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((CommonServiceModel)item).CommonServiceName" label="Tên dịch vụ" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((CommonServiceModel)item).Price" label="Đơn giá" />
        </div>
        <div class="form-group col-md-12">
            <az-text-area-model model="Model.Data" func="(item)=>((CommonServiceModel)item).Note" label="Ghi chú" />
        </div>
    </div>
<az-script>

</az-script>