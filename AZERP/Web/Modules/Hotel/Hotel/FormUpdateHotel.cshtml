@model FormUpdateHotel
    <div class="az-update row" id="FormUpdateHotel" role="form">
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.HotelModel)item).HotelID" label="Mã phòng trọ" attr="autofocus" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.HotelModel)item).HotelName" label="Tên phòng trọ" />
        </div>
        <div class="form-group col-md-6">
            <az-typeOfHotel-select model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.HotelModel)item).TypeOfHotelID" label="Loại phòng trọ" />
        </div>
        <div class="form-group col-md-6">
            <az-area-select model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.HotelModel)item).AreaID" label="Khu vực"/>
        </div>
        <div class="form-group col-md-6">
            <az-hotel-status-select model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.HotelModel)item).HotelStatus" label="Trạng thái" />
        </div>
    </div>
<az-script>

</az-script>