@model FormUpdateUnitPrice
    <div class="az-update row" id="FormUpdateUnitPrice" role="form">
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.UnitPriceModel)item).UnitPriceID" label="Mã đơn giá" attr="autofocus" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.UnitPriceModel)item).ElectricityCharge" label="Tiền điện" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.UnitPriceModel)item).WaterCharge" label="Tiền nước" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.UnitPriceModel)item).ChargeOther" label="Tiền khác" />
        </div>
        <div class="form-group col-md-12">
            <az-text-area-model model="Model.Data" func="(item)=>((AZERP.Data.Entities.Hotel.UnitPriceModel)item).Note" label="Ghi chú" />
        </div>
    </div>
<az-script>

</az-script>