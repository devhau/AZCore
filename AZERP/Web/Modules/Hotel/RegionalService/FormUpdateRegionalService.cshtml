@model FormUpdateRegionalService
    <div class="az-update row" id="FormUpdateRegionalService" role="form">
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RegionalServiceModel)item).RegionalServiceID" label="Mã dịch vụ" attr="autofocus" />
        </div>
        <div class="form-group col-md-6">
            <az-common-service-select model="Model.Data" func="(item)=>((RegionalServiceModel)item).CommonServiceID" label="Tên dịch vụ" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RegionalServiceModel)item).Price" label="Đơn giá" />
        </div>
        <div class="form-group col-md-12">
            <az-text-area-model model="Model.Data" func="(item)=>((RegionalServiceModel)item).Note" label="Ghi chú" />
        </div>
    </div>
<az-script>

</az-script>