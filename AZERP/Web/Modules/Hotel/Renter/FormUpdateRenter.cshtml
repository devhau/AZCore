@model FormUpdateRenter
    <div class="az-update row" id="FormUpdateRenter" role="form">
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RenterModel)item).RenterCode" label="Mã Người Thuê" attr="autofocus" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RenterModel)item).RenterName" label="Tên Người Thuê" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RenterModel)item).Address" label="Địa Chỉ" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RenterModel)item).CMND" label="Số CMND/CCCD" />
        </div>
        <div class="form-group col-md-6">
            <az-text-model model="Model.Data" func="(item)=>((RenterModel)item).Tel" label="Số Điện Thoại" />
        </div>
        <div class="form-group col-md-12">
            <az-text-area-model model="Model.Data" func="(item)=>((RenterModel)item).Note" label="Ghi chú" />
        </div>
    </div>
<az-script>

</az-script>