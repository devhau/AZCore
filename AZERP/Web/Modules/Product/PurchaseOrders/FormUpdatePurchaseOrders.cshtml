@model FormUpdatePurchaseOrders
    <div class="az-update row" id="FormUpdatePurchaseOrders" role="form">
        <div class="form-group col-md-12">
            <az-text-model model="Model.Data" func="(item)=>((PurchaseOrderModel)item).Code" label="Mã đơn nhập hàng" />
        </div>
        <div class="form-group col-md-12">
            <az-supplier-select model="Model.Data" func="(item)=>((PurchaseOrderModel)item).SupplierCode" label="Nhà cung cấp" />
        </div>
        <div class="form-group col-md-12">
            <az-product-select model="Model.Data" func="(item)=>((PurchaseOrderModel)item).ProductCode" label="Sản phẩm" />
        </div>
    </div>
<az-script>

</az-script>