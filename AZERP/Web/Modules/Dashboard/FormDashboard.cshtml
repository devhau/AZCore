@model FormDashboard
<az-script module="Dashboard.js" />
<az-style module="Dashboard.css" />
<az-module-layout buttons="Model.GetButtonInfo()" icon="fas fa-tachometer-alt" script-random='(t)=>$"DashboardForm(\".{t}\")"'>
    <div class="az-dashboard container-fluid">
        <div class="row">
            @foreach (var item in Model.Widgets())
            {
                @item.GetContent();
            }
        </div>
    </div>
</az-module-layout>